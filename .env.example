# Database Configuration
DATABASE_URL=postgresql+psycopg2://username:password@localhost/duksu

# LLM Configuration
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GEMINI_API_KEY=

# Logging
LOG_LEVEL=info

# Object Store Configuration
# Options: 'local' or 's3'
OBJECT_STORE_TYPE=local

# Local File System Object Store
OBJECT_STORE_BASE_PATH=storage/objects

# S3 Object Store (only needed if OBJECT_STORE_TYPE=s3)
S3_BUCKET_NAME=your-bucket-name
S3_REGION=us-east-1
S3_ACCESS_KEY_ID=your-access-key-id
S3_SECRET_ACCESS_KEY=your-secret-access-key
# Optional: for S3-compatible services like MinIO
# S3_ENDPOINT_URL=http://localhost:9000